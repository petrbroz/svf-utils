{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Manifest",
    "type": "object",
    "properties": {
        "guid": {
            "type": "string",
            "description": "Globally unique identifier for the manifest."
        },
        "owner": {
            "type": "string",
            "description": "Identifier for the owner of the manifest."
        },
        "hasThumbnail": {
            "type": "string",
            "enum": [
                "true",
                "false"
            ],
            "description": "Indicates whether a thumbnail is available."
        },
        "startedAt": {
            "type": "string",
            "description": "Start time for the manifest processing."
        },
        "type": {
            "type": "string",
            "description": "Type of the manifest, e.g., 'design'."
        },
        "urn": {
            "type": "string",
            "description": "Unique resource identifier for the manifest."
        },
        "success": {
            "type": "string",
            "description": "Processing success percentage, e.g., '100%'."
        },
        "progress": {
            "type": "string",
            "description": "Processing progress, e.g., 'complete'."
        },
        "region": {
            "type": "string",
            "description": "Region where the processing occurred."
        },
        "status": {
            "type": "string",
            "description": "Overall status of the manifest, e.g., 'success'."
        },
        "registerKeys": {
            "type": "object",
            "description": "Map of command IDs to related keys.",
            "additionalProperties": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            }
        },
        "children": {
            "type": "array",
            "description": "Nested structure containing children elements.",
            "items": {
                "$ref": "#/definitions/Child"
            }
        }
    },
    "required": [
       
        
        "guid",
        "hasThumbnail",
        "type",
        "urn",
        "success",
        "progress",
        "status",
        "children"
    ],
    "definitions": {
        "Child": {
            "type": "object",
            "properties": {
                "guid": {
                    "type": "string",
                    "description": "Globally unique identifier for the child."
                },
                "role": {
                    "type": "string",
                    "description": "Role of the child, e.g., 'viewable'."
                },
                "hasThumbnail": {
                    "type": "string",
                    "enum": [
                        "true",
                        "false"
                    ],
                    "description": "Indicates whether a thumbnail is available."
                },
                "progress": {
                    "type": "string",
                    "description": "Processing progress, e.g., 'complete'."
                },
                "type": {
                    "type": "string",
                    "description": "Type of the child, e.g., 'folder'."
                },
                "status": {
                    "type": "string",
                    "description": "Overall status of the child, e.g., 'success'."
                },
                "version": {
                    "type": "string",
                    "description": "Version of the child element."
                },
                "urn": {
                    "type": "string",
                    "description": "Unique resource identifier for the child."
                },
                "inputFileSize": {
                    "type": "number",
                    "description": "Size of the input file in bytes."
                },
                "inputFileType": {
                    "type": "string",
                    "description": "Type of the input file, e.g., 'rvt'."
                },
                "name": {
                    "type": "string",
                    "description": "Name of the child element."
                },
                "properties": {
                    "$ref": "#/definitions/Properties",
                    "description": "Properties associated with the child."
                },
                "otg_manifest": {
                    "$ref": "#/definitions/OTGManifest",
                    "description": "OTG manifest information for the child."
                },
                "children": {
                    "type": "array",
                    "description": "Nested children elements.",
                    "items": {
                        "$ref": "#/definitions/Child"
                    }
                }
            },
            "required": [
                "guid",
                "type"
            ]
        },
        "Properties": {
            "type": "object"
        },
        "OTGManifest": {
            "type": "object",
            "properties": {
                "version": {
                    "type": "number"
                },
                "creator": {
                    "type": "string"
                },
                "first_started_at": {
                    "type": "string"
                },
                "last_started_at": {
                    "type": "string"
                },
                "last_modified_at": {
                    "type": "string"
                },
                "invocations": {
                    "type": "number"
                },
                "status": {
                    "type": "string"
                },
                "success": {
                    "type": "string"
                },
                "progress": {
                    "type": "string"
                },
                "urn": {
                    "type": "string"
                },
                "pdb_manifest": {
                    "$ref": "#/definitions/PDBManifest"
                },
                "views": {
                    "type": "object",
                    "additionalProperties": {
                        "$ref": "#/definitions/View"
                    }
                },
                "account_id": {
                    "type": "string"
                },
                "paths": {
                    "$ref": "#/definitions/Paths"
                }
            },
            "required": [
                "paths",
                "views"
            ]
        },
        "PDBManifest": {
            "type": "object",
            "properties": {
                "pdb_version_rel_path": {
                    "type": "string"
                },
                "pdb_shared_rel_path": {
                    "type": "string"
                },
                "assets": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Asset"
                    }
                }
            }
        },
        "Asset": {
            "type": "object",
            "properties": {
                "uri": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "isShared": {
                    "type": "boolean"
                }
            }
        },
        "View": {
            "type": "object",
            "properties": {
                "role": {
                    "type": "string"
                },
                "mime": {
                    "type": "string"
                },
                "urn": {
                    "type": "string"
                }
            },
            "required": [
                "role",
                "mime",
                "urn"
            ]
        },
        "Paths": {
            "type": "object",
            "properties": {
                "version_root": {
                    "type": "string"
                },
                "shared_root": {
                    "type": "string"
                },
                "global_root": {
                    "type": "string"
                },
                "global_sharding": {
                    "type": "number"
                },
                "region": {
                    "type": "string"
                }
            },
            "required": [
                "version_root",
                "shared_root",
                "global_root",
                "global_sharding",
                "region"
            ]
        }
    }
}